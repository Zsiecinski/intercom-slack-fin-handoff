{
  "name": "intercom-slack-fin-handoff",
  "version": "2.0.0",
  "type": "module",
  "description": "Polling service to notify Slack when Intercom tickets are assigned to admins",
  "main": "src/poll.js",
  "scripts": {
    "start": "node src/poll.js",
    "start:commands": "node src/command-server.js",
    "start:dashboard": "node src/dashboard-server.js",
    "start:all": "concurrently \"npm run start\" \"npm run start:commands\"",
    "dev": "node --watch src/poll.js",
    "poll": "node src/poll.js",
    "test": "node src/test-poll.js",
    "test-once": "node src/test-poll-once.js",
    "test-tokens": "node src/test-tokens.js"
  },
  "keywords": [
    "intercom",
    "slack",
    "tickets",
    "polling"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
}

